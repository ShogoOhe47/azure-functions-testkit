{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "myOutputBlob",
      "type": "blob",
      "path": "functions-testkit-blob/{DateTime}.txt",
      "connection": "BlobConnectionString",
      "direction": "out"
    }
  ]
}