{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "eventHub",
      "name": "outputEventHubMessage",
      "eventHubName": "EventHubName",
      "connection": "EventHubConnectionString",
      "direction": "out"
    }
  ]
}